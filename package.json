    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "jest --no-cache",
    "tsc": "tsc",
    "dev": "tsc -w"
  },
  "author": "robert.kami88@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "jest": "^26.2.2",
    "node-fetch": "^2.6.0",
    "ts-jest": "^26.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "tslint": "^5.12.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robert8888/Rpn.git"
  },
  "keywords": [
    "rpn",
    "calculator",
    "rever",
    "polish",
    "notation",
    "matematical",
    "calcuator"
  ],
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "import":  "./build/esm/index.js"
    }
  },
  "bugs": {
